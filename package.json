{
  "name": "ng-md-starter",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build --aot",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "e2e": "ng e2e",
    "ng": "ng",
    "postinstall": "ng build --aot --prod",
    "pretty": "prettier --parser typescript --loglevel warn --write \"src/**/*.ts\"",
    "serve:local": "ng serve --aot --port 4203 --configuration=local --open",
    "serve:prod": "ng serve --aot --port 4203 --configuration=prod",
    "serve:ngrok": "ngrok http 4204",
    "start": "node server.js",
    "test": "ng test --code-coverage --watch=true"
  },
  "precommit": [
    "pretty",
    "lint",
    "postinstall"
  ],
  "engines": {
    "node": "9.11.1",
    "npm": "6.8.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/forms": "^7.2.6",
    "@angular/http": "^7.2.6",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@angular/router": "^7.2.6",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-scss-preprocessor": "^4.0.0",
    "post-commit": "0.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "1.16.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-defocus": "^2.0.6",
    "typescript": "~3.2.2"
  }
}
